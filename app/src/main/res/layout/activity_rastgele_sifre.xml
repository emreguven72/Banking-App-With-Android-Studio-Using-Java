<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E61313"
    tools:context=".RastgeleSifreActivity">

    <Button
        android:id="@+id/sifreDegistirOnaylaButton2"
        android:layout_width="290dp"
        android:layout_height="50dp"
        android:layout_marginBottom="160dp"
        android:onClick="goBack"
        android:text="Geri Dön"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.479"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/rastgeleSifreSifreText"
        android:layout_width="286dp"
        android:layout_height="29dp"
        android:layout_marginBottom="100dp"
        android:background="#000000"
        android:ems="10"
        android:inputType="number"
        android:textColor="#FFFFFF"
        android:clickable="false"
        android:focusableInTouchMode="false"
        android:focusable="false"
        android:cursorVisible="false"
        app:layout_constraintBottom_toTopOf="@+id/sifreDegistirOnaylaButton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/eskiSifre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="Rastgele Şifre"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/rastgeleSifreSifreText"
        app:layout_constraintStart_toStartOf="@+id/rastgeleSifreSifreText" />
</androidx.constraintlayout.widget.ConstraintLayout>